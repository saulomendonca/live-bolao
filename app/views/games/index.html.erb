<h1>Listing games</h1>

<table>
  <thead>
    <tr>
      <th>Web service</th>
      <th>Vippredictor</th>
      <th>Status</th>
      <th>Complete</th>
      <th>Weight</th>
      <th>Date</th>
      <th>Away team</th>
      <th>Home team</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @games.each do |game| %>
      <tr>
        <td><%= game.webservice_id %></td>
        <td><%= game.vippredictor_id %></td>
        <td><%= game.status %></td>
        <td><%= game.complete %></td>
        <td><%= game.weight %></td>
        <td><%= game.date %></td>
        <td><%= game.away_team %></td>
        <td><%= game.home_team %></td>
        <td><%= link_to 'Show', game %></td>
        <td><%= link_to 'Edit', edit_game_path(game) %></td>
        <td><%= link_to 'Destroy', game, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Game', new_game_path %>
